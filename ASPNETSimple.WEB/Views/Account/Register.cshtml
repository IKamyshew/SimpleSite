@model ASPNETSimple.WEB.Models.Account.RegisterViewModel

@{
    ViewBag.Title = "Register";
}

@using (Html.BeginForm("Register", "Account", null, FormMethod.Post, new { @class = "", role = "form" }))
{
    @Html.ValidationSummary(true, "", new { @class = "" })
    <div class="">@Html.TextBoxFor(m => m.Email, new { @class = "", tabindex = "1", autofocus = "true", id = "email-input" })</div>
    <div>@Html.ValidationMessageFor(m => m.Email, "", new { @class = "" })</div>
    <div class="">@Html.PasswordFor(m => m.Password, new { @class = "", tabindex = "2", id = "password-input" })</div>
    <div>@Html.ValidationMessageFor(m => m.Password, "", new { @class = "" })</div>
    <div class="">@Html.PasswordFor(m => m.ConfirmPassword, new { @class = "", tabindex = "3", id = "confirm-password-input" })</div>
    <div>@Html.ValidationMessageFor(m => m.ConfirmPassword, "", new { @class = "" })</div>
    <div class="">@Html.TextBoxFor(m => m.FirstName, new { @class = "", tabindex = "4", id = "first-name-input" })</div>
    <div>@Html.ValidationMessageFor(m => m.FirstName, "", new { @class = "" })</div>
    <div class="">@Html.TextBoxFor(m => m.LastName, new { @class = "", tabindex = "5", id = "last-name-input" })</div>
    <div>@Html.ValidationMessageFor(m => m.LastName, "", new { @class = "" })</div>
    <input type="submit" value="Log in" class="" tabindex="6" />
}