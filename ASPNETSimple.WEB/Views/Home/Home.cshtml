@model IEnumerable<ASPNETSimple.DAL.Entities.UserProfile>

@{
    ViewBag.Title = "Home";
}

<style>
    label {
        font-weight: normal;
        margin: 0;
        margin-right: 50px;
    }
</style>

<h2>Home</h2>
@foreach (ASPNETSimple.DAL.Entities.UserProfile profile in Model)
{
    <div>
        <label><strong>ID:</strong> @profile.Id</label>
        <label><strong>Name:</strong> @profile.Name</label>
        <label><strong>Address:</strong> @profile.Address</label>
    </div>
}

